<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="RadiusTextStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:gravity">center</item>
        <item name="android:minHeight">45dp</item>
        <item name="android:textSize">14dp</item>
        <item name="android:elevation">2dp</item>
        <item name="com.aries.library.fast.demo:rv_radius">@dimen/dp_radius</item>
        <item name="com.aries.library.fast.demo:rv_backgroundColor">@color/colorWhite</item>
        <item name="com.aries.library.fast.demo:rv_backgroundPressedColor">
            @color/colorWhitePressed
        </item>
        <item name="com.aries.library.fast.demo:rv_strokeColor">@color/colorLineGray</item>
        <item name="com.aries.library.fast.demo:rv_strokeWidth">@dimen/dp_line_size</item>
    </style>
</resources>